<div class="edit">
  <div class="container-form">
    <h1>Edit your meeting</h1>
    <h2>In this meeting <%= @meeting.user.name %></h2>
    <%= simple_form_for (@meeting) do |f| %>
      <%= f.input :start_date  %>
      <%= f.input :duration %>
      <%= f.input :location %>
      <%= f.submit 'Submit', class:"btn btn-primary" %>
    <% end %>
    <%= link_to "Back", meetings_path %>
  </div>
</div>
