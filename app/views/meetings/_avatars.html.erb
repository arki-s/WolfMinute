 <% if users.length >= 1 %>
  <% users.each do |user|%>
    <%= image_tag user.photo.key ? cl_image_path(user.photo.key) :  "https://kitt.lewagon.com/placeholder/users/ssaunier", class: "avatar"%>
    <%= link_to meetings_path(user_id: user.id, query: params[:query]), class:"text-decoration-none" do %>
      <%= user.name%>
    <% end %>
  <% end %>
<% end %>
